
#include<stdio.h>
#define N 20
int main()
{
    char str[N];
    int i,temp,len;
    printf("enter the str:");
    scanf("%s",str);
    for(len=0;str[len]!='\0';len++);
    for(i=0;i<len/2;i++)
    {
        temp=str[i];
        str[i]=str[len-i-1];
        str[len-i-1]=temp;
    }
    printf("%s",str);
        
    
    
}
