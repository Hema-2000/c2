// Online C compiler to run C program online
#include <stdio.h>
#include<string.h>
int main() {
   char str[]="hemalatha";
   int i,j;
   int len=strlen(str);
   int s[len];
   for(i=0;i<len;i++)
   {
        int count=1;
       for(j=i+1;j<len;j++)
       {
         
           if(str[i]==str[j])
           {
           count++;
           s[j]=-1;
           }
       }
       if(s[i]!=-1)
       s[i]=count;
   }
   for(i=0;i<len;i++)
   {
        if(s[i]!=-1)
       printf("%c element freq is %d\n",str[i],s[i]);
   }
   
   

    return 0;
}
