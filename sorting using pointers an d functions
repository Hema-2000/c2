#include<stdio.h>
int *swapping(int *ptr,int n)
{
   int i,j,temp;
   for(i=0;i<n;i++)
   {
       for(j=0;j<n;j++)
       {
           if(ptr[i]<ptr[j])
           {
               temp=ptr[i];
               ptr[i]=ptr[j];
               ptr[j]=temp;
           }

       }
   }
   
  return ptr;
}
int main()
{
    int arr[5],i;
    int *ptr=arr;
    printf("enter the elements in an array:");
    for(i=0;i<5;i++)
    scanf("%d",&ptr[i]);
    for(i=0;i<5;i++)
    printf("%d",ptr[i]);
    swapping(ptr,5);
    printf("\n");
   for(i=0;i<5;i++)
   printf("%d ",ptr[i]);
    return 0;
}
