#include<stdio.h>
#include<stdlib.h>
#include<string.h>
//#define N 5
typedef struct prog
{
    char str[10];
    int a;
}s;
char reverse(char str1[],int len,int n)
{
    int i,temp;
    for(i=0;i<n/2;i++)
    {
        temp=str1[len-(n-i)];
        str1[len-(n-i)]=str1[len-(i+1)];
        str1[len-(i+1)]=temp;
    }
  
  return str1;
}
int main()
{
  s *s1,*s2;
  s1=(s*)malloc(sizeof(s));
   s2=(s*)malloc(sizeof(s));
   int n;
   printf("enter the s1 details:");
   scanf("%s %d",s1->str,&s1->a);
   printf("enter the s2 details:");
   scanf("%s %d",s2->str,&s2->a);
  
   
   if(((strcmp(s1->str,s2->str))==0) && (s1->a==s2->a))
   { 
       char str1[10];
       printf("enter the string:");
       scanf("%s",str1);
       int len=strlen(str1);
        printf("enter the value of n:");
        scanf("%d",&n);
       if(len<n)
       {
           printf("enter n value less than len");
       }
       else
       {
       char (*ptr)(char[],int,int);
       ptr=reverse;
       (*ptr)(str1,len,n);
       printf("%s",str1);
       }
   } 
  
  else
   {
     
      unsigned char k;
      printf("enter the char:");
       scanf(" %c",&k);
       char temp1=(k<<4);
       char temp2=(k>>4);
       char res=temp1|temp2;
       printf("%d",res);
    return 0;
   }
}
  
