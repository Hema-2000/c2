#include<stdio.h>
#include<string.h>
char find(char str[])
{
    static int i=0;
    while(i<strlen(str))
    {
        if(str[i]>=65 && str[i]<=90)
        {
           return str[i];
           
        }
        i=i+1;
        return find(str);
       
    }
    
}
int main()
{
    char str[]="hemaLatha";
    char a;
    a=find(str);
    
 printf("%c",a);
    return 0;
}
