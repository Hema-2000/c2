#include<stdio.h>
int fun(int[],int size);
int i;
int main()
{
    int arr[10];
    int size;
    printf("enter the size of arr:");
    scanf("%d",&size);
    printf("enter the array elements:");
    for(i=0;i<size;i++)
    {
        scanf("%d",&arr[i]);
    }
    int l=fun(arr,size);
    size++;
        printf("%d",l);
}
int fun(int arr[],int size)
{
    int sum=0,n=size+1,i,j;
    for(i=0;i<size;i++)
    {
        sum=sum+arr[i];
    }
    j=(n*(n+1))/2 - sum;
    return j;
}





#include<stdio.h>
int fun(int[],int size);
int i;
int main()
{
    int arr[10];
    int size;
    printf("enter the size of arr:");
    scanf("%d",&size);
    printf("enter the array elements:");
    for(i=0;i<size;i++)
    {
        scanf("%d",&arr[i]);
    }
    int l=fun(arr,size);
    size++;
        printf("%d",l);
}
int fun(int arr[],int size)
{
   int temp[size+1],ans;
   for(i=0;i<=size;i++)
   {
       temp[i]=0;
   }
   for(i=0;i<size;i++)
   {
       temp[arr[i]-1]=1;
   }
   for(i=0;i<=size;i++)
   if(temp[i]==0)
   {
       ans=i+1;
   }
   
   return ans;
}
