#include<stdio.h>
int main()
{
    int arr[]={1,3,8,5,5,1};
    int size=sizeof(arr)/sizeof(int);
   // int b[size];
    int i,j;
     for(i=0; i<size; i++)
   {
    for(j=i+1;j<size;j++)
    {
    if(arr[i]==arr[j])
    printf("%d\n",arr[i]);
   }
   }
}  



to eliminate duplicate elements
#include<stdio.h>
int main()
{
    int arr[]={1,3,8,5,5,1};
    int size=sizeof(arr)/sizeof(int);
    int i,j,k;
     for(i=0; i<size; i++)
   {
    for(j=i+1;j<size;j++)
    {
    if(arr[i]==arr[j])
    {
        for(k=j;k<size-1;k++)
         {
             arr[k]=arr[k+1];
        }
         size--;
    }
    
   }
   }
     for(i=0; i<size; i++)
     printf("%d ",arr[i]);
}  
