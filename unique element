#include<stdio.h>
int main()
{
    int arr[8]={1,1,1,3,3,8,5,5};
    int size=sizeof(arr)/sizeof(int);
    int b[size];
    int i,j;
    for(i=0;i<size;i++)
    {
        for( j=0;j<size;j++)
        {
            if(arr[i]==arr[j] && i!=j)
            
           break;
        }
            if(j==size)
            printf("%d",arr[i]);
    }
        
}
