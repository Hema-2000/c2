#include<stdio.h>
#include<string.h>
char str_cpy(char str1[],char str2[])
{
    int i;
    for(i=0;str2[i]!='\0';i++)
    {
        str1[i]=str2[i];
    }
    str1[i]='\0';
}
int main()
{
    char str1[20];
    char str2[20];
    printf("enter the str2");
    scanf("%s",str2);
    
     str_cpy(str1,str2);
    printf("after copying str1:%s str2:%s\n ",str1,str2);
    return 0;
}
