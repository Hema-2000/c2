 #include<stdio.h>
 #include<stdlib.h>
 struct node
 {
     int data;
     struct node *next;
 };
 struct node *head=NULL;
 void addnode(int data)
 {
     struct node *newnode=(struct node*)malloc(sizeof(struct node));
    
     newnode->data=data;
     newnode->next=NULL;
     if(head==NULL)
     head=newnode;
     else
     {
          
     struct node *temp=head;
     while(temp->next!=NULL)
     {
         temp=temp->next;
     }
     temp->next=newnode;
     }
 }
 void display()
 {
     
     struct node *temp=head;
     while(temp!=NULL)
     {
         printf("%d ",temp->data);
         temp=temp->next;
     }
 }
 void swap()
 {
     struct node *temp=head,*prev=NULL,*current=NULL;
     if(head==NULL)
     return;
     else
     {
     while(temp->next!=NULL)
     {
         prev=temp;
         temp=temp->next;
     }
     }
     if(head==temp)
     return;
     else if(head->next==temp)
     {
         current=head;
         head=temp;
         head->next=current;
         current->next=NULL;
         
     }
     else
     {
         current=head;
         head=temp;
         head->next=current->next;
         prev->next=current;
         current->next=NULL;
     }
 }
 int main()
 {
     struct node *head;
     addnode(1);
     addnode(2);
     addnode(3);
     addnode(4);
     display();
     swap();
     display();
 }
