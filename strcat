#include <stdio.h>
#include<string.h>
char str_concat(char str1[],char str2[])
{
    int l1=strlen(str1);
    int l2=strlen(str2);
    int i;
    for(i=0;str2[i]!='\0';i++)
    {
        str1[l1+i]=str2[i];
    }
    
    str1[l1+i]='\0';
}
int main()
{
    char str1[20];
    char str2[20];
    printf("enter the str1: ");
    scanf("%s",str1);
    
     printf("enter the str2: ");
    scanf("%s",str2);
   
   
     str_concat(str1,str2);
    printf("%s",str1);
    return 0;
}
