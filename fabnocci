#include<stdio.h>

int main()
{
    int val;
    printf("enter the value: ");
    scanf("%d",&val);
    int n1=0,n2=1;
    int n3=n1+n2;
    printf("%d %d ",n1,n2);
    for(int i=3;i<val;i++)
    {  
         printf("%d ",n3); 
        n1=n2;
        n2=n3;
        n3=n1+n2;
      
    }
    
    return 0;
}
