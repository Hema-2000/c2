#include<stdio.h>
char *str_chr(char str1[])
{
    int i;
    for(i=0;str1[i]!='\0';i++)
    {
        if(str1[i]>=97 && str1[i]<=122)
        {
            str1[i]=str1[i]-32;
        }
    }
    return str1;
}
int main()
{
    char str1[20]="hemalatha";
    str_chr(str1);
    printf("%s",str1);
}
