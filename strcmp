#include<stdio.h>
#include<string.h>
int str_cmp(char *str1,char *str2)
{
    int i,count=0;
    for(i=0;str1[i]!='\0'||str2[i]!='\0';i++)
    {
        if(str1[i]!=str2[i])
        count=1;
        break;
    }
    (count==1)?printf("strings are not equal"):printf("strings are equal");
    
}
int main()
{
    char str1[20];
    char str2[20];
    printf("enter the str1");
    scanf("%s",str1);
    printf("enter the str2");
    scanf("%s",str2);
     str_cmp(str1,str2);
   
    return 0;
}
