#include<stdio.h>
int selection(int arr[],int n)
{
    int i,j,temp;
    for(i=0;i<n;i++)
    {
        for(j=i+1;j<n;j++)
        {
            if(arr[i]>arr[j])
            {
              temp=arr[i];
              arr[i]=arr[j];
              arr[j]=temp;
            }
        }
    }
    for(i=0;i<n;i++)
    printf("%d ",arr[i]);
}
int main()
{
    int arr[10];
    int n;
    printf("enter the size of an arr:");
    scanf("%d",&n);
    printf("enter the elements in an array:");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    int (*fptr)(int[],int n);
    fptr=selection;
    fptr(arr,n);
}
