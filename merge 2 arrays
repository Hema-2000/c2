#include<stdio.h>
#include<stdlib.h>
int freq_ele(int *p1,int *p2,int n1,int n2)
{
    int n3=n1+n2;
    int i,j;
    int *p3=(int *)malloc(n3*sizeof(int));
    for(i=0;i<n1;i++)
    {
       *(p3+i)=*(p1+i);
      
    }
    j=i;
    for(i=0;i<n2;i++)
    {
        *(p3+j)=*(p2+i);
        j++;
    }
    for(i=0;i<n3;i++)
    {
        printf("%d ",*(p3+i));
    }
}
int main()
{
    
    int n1,n2,i;
    printf("enter the size of an array:");
    scanf("%d %d",&n1,&n2);
    int *ptr1=(int *)malloc(n1*sizeof(int));
    int *ptr2=(int *)malloc(n2*sizeof(int));
    printf("enter the 1st array elements:");
    for(i=0;i<n1;i++)
        scanf("%d",ptr1+i);
        printf("enter the 2nd array elements:");
    for(i=0;i<n2;i++)
        scanf("%d",ptr2+i);
    int (*fptr)(int *,int *,int,int);
    fptr=freq_ele;
    fptr(ptr1,ptr2,n1,n2);
}
